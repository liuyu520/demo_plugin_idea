<idea-plugin>
    <id>com.kunlunsoft.demo_plugin_idea</id>
    <name>my_plugin</name>
    <version>0.0.1</version>
    <vendor email="dummy" url="dummy">dummy</vendor>

    <description><![CDATA[
      Sample plugin.<br>
    ]]></description>

    <change-notes><![CDATA[
      Release 0.0.1: Initial release.<br>
    ]]>
    </change-notes>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="162"/>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    <depends>com.intellij.modules.lang</depends>
    -->

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow id="My Sample Tool Window" icon="/logo/toolWind.png" anchor="right"
                    factoryClass="com.kunlunsoft.dialog.MyToolWindow"/>
        <!--<moduleBuilder builderClass="com.kunlunsoft.project.wizard.DemoModuleWizardStep" id="DEMO_STEP22"
                       order="first"/>
        <moduleBuilder builderClass="com.kunlunsoft.project.wizard.DemoModuleWizardSecondStep" id="DEMO_STEP22Second2"
                       order="after DEMO_STEP22"/>-->

        <moduleBuilder builderClass="com.kunlunsoft.project.wizard.better.DemoModuleBuilder" id="DEMO_STEP22Second2"
                       order="first"/>
        <!--项目类型-->
        <moduleType id="DEMO_MODULE_TYPE" implementationClass="com.kunlunsoft.project.wizard.better.DemoModuleType"/>

    </extensions>

    <actions>
        <group id="MyPlugin.SampleMenu" text="Greeting主菜单栏" description="Greeting menu">
            <add-to-group group-id="MainMenu" anchor="last"/>
            <action id="Myplugin.Textboxes" class="com.kunlunsoft.HelloAction" text="Hello" description="Says hello"/>
            <action id="ToolWindowDemo" class="com.kunlunsoft.menu.WindowMainMenuDemo"
                    text="ToolWindowToolbarsDemo name" description="ToolWindowToolbarsDemo desc"/>
        </group>

    </actions>

</idea-plugin>